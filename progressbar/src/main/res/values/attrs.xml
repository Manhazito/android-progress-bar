        <attr name="initialStage" format="integer"/>
        <attr name="draggableMarkers" format="boolean"/>
        <attr name="lightColor" format="color"/>
        <attr name="darkColor" format="color"/>
        <attr name="divisions" format="integer"/>
        <attr name="markerCentered" format="boolean"/>
